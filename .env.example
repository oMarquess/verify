# Frontend Environment Variables
# Copy this file to .env.local and update the values for your environment

# Liveness Service WebSocket URL
NEXT_PUBLIC_LIVENESS_WS_URL=ws://localhost:8000/stream-liveness

# Verification Service API URL  
NEXT_PUBLIC_VERIFICATION_API_URL=http://localhost:8001/session-verify

# For production, use secure protocols:
# NEXT_PUBLIC_LIVENESS_WS_URL=wss://your-domain.com/stream-liveness
